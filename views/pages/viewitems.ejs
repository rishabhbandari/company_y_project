<style>
  table {
    border-collapse: collapse;
    width: 100%;
  }

  th,
  td {
    text-align: left;
    padding: 8px;
  }

  tr:nth-child(even) {
    background-color: #f2f2f2;
  }

  th {
    background-color: #4caf50;
    color: white;
  }
</style>


<body>
  <h1>View Your item Data</h1>
  </br>
  <form action="/">
    <button>Home</button>
  </form>
  </br>
  <table>
    <thread>
      <tr>
        <th>item Name</th>
        <th>item Description</th>
        <th>item Price</th>
        <th>item Quantity</th>
      </tr>
    </thread>
    <tbody>
      <% items.forEach(function(item){ %>
      <tr>
        <td><%= item.item_name %></td>
        <td><%= item.item_description %></td>
        <td><%= item.item_price %></td>
        <td><%= item.item_quantity %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
  </br>
  </br>
  
</body>